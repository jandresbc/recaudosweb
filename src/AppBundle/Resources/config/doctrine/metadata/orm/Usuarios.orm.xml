<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Usuarios" table="usuarios">
    <indexes>
      <index name="id_grupo_usuario" columns="id_grupo_usuario"/>
      <index name="usuarios_ibfk_2" columns="id_empresa_sede_agencia"/>
    </indexes>
    <id name="idUsuario" type="integer" column="id_usuario">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombreCompleto" type="string" column="nombre_completo" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="identificacion" type="integer" column="identificacion" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="contrasena" type="string" column="contrasena" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="telefono" type="integer" column="telefono" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="activo" type="integer" column="activo" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">1= Activo, 0=Inactivo</option>
        <option name="default">1</option>
      </options>
    </field>
    <field name="auth" type="integer" column="auth" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">1=Autenticado, 0= No Autenticado</option>
        <option name="default">0</option>
      </options>
    </field>
    <many-to-one field="idGrupoUsuario" target-entity="GruposUsuarios" fetch="LAZY">
      <join-columns>
        <join-column name="id_grupo_usuario" referenced-column-name="id_grupo_usuario"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idEmpresaSedeAgencia" target-entity="EmpresasSedesAgencias" fetch="LAZY">
      <join-columns>
        <join-column name="id_empresa_sede_agencia" referenced-column-name="id_empresa_sede_agencia"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
