<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Divipola" table="divipola">
    <id name="divipola" type="integer" column="divipola">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="codDepto" type="integer" column="cod_depto" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="codMpio" type="integer" column="cod_mpio" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="depto" type="string" column="depto" length="30" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nomPoblad" type="string" column="nom_poblad" length="30" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
